{
  "name": "rust-recipes-e2e",
  "version": "0.1.0",
  "private": true,
  "dependencies": {},
  "scripts": {
    "start:ui": "yarn --cwd ../ui start",
    "wait-on": "wait-on https://localhost:3000 && yarn cypress run",
    "test": "concurrently --kill-others --success first \"yarn run start:ui\" \"yarn run wait-on\""
  },
  "eslintConfig": {},
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "concurrently": "^7.0.0",
    "cypress": "^9.2.1",
    "wait-on": "^6.0.0"
  }
}
